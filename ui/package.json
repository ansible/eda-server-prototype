{
  "name": "eda-server-ui",
  "version": "0.0.1",
  "description": "User interface for ansible events.",
  "repository": "https://github.com/ansible/eda-server.git",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "npm run clean",
    "dr:surge": "node dr-surge.js",
    "build": "webpack --config config/webpack.prod.js && npm run dr:surge",
    "start": "sirv dist --cors --single --host --port 8080",
    "start:dev": "webpack serve --color --progress --config config/webpack.dev.js",
    "test": "jest --watch",
    "test:coverage": "jest --coverage",
    "eslint": "eslint --ext .tsx,.js ./src/",
    "lint": "npm run eslint",
    "format": "prettier --check --write ./src/**/*.{tsx,ts}",
    "type-check": "tsc --noEmit",
    "ci-checks": "npm run type-check && npm run lint && npm run test:coverage",
    "build:bundle-profile": "webpack --config config/webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "npm run build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook",
    "travis:build": "NODE_ENV=production webpack --config config/webpack.prod.js --mode=production",
    "travis:verify": "npm-run-all travis:build lint test",
    "test": "jest --passWithNoTests --runInBand",
    "verify": "npm-run-all build lint test"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-info": "^3.0.0",
    "@storybook/addon-knobs": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addons": "^6.2.9",
    "@storybook/react": "^6.1.21",
    "@types/enzyme": "^3.10.12",
    "@types/jest": "^28.1.5",
    "@types/react-router-dom": "^5.1.7",
    "@types/victory": "^33.1.4",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "copy-webpack-plugin": "^9.0.0",
    "css-minimizer-webpack-plugin": "^3.0.1",
    "detect-browser": "^5.3.0",
    "dotenv-webpack": "^7.0.3",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.28.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "imagemin": "^8.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-fetch-mock": "^3.0.3",
    "mini-css-extract-plugin": "^1.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.1",
    "prettier": "^2.3.1",
    "prop-types": "^15.7.2",
    "raw-loader": "^4.0.2",
    "react-axe": "^3.5.4",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-json-view": "^1.21.3",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^7.1.1",
    "terser-webpack-plugin": "^5.1.3",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.2.3",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@patternfly/react-core": "^4.225.9",
    "@patternfly/react-icons": "^4.75.1",
    "@patternfly/react-styles": "^4.74.1",
    "@patternfly/react-table": "^4.93.1",
    "@redhat-cloud-services/frontend-components": "^3.9.8 ",
    "@redhat-cloud-services/frontend-components-notifications": "^3.2.9",
    "@storybook/builder-webpack5": "^6.2.9",
    "ansi-to-react": "^6.1.6",
    "css-loader": "^6.7.1",
    "react": "^17.0.2",
    "react-ace": "^10.1.0",
    "react-dom": "^17.0.2",
    "react-intl": "^6.0.5",
    "react-table": "^7.8.0",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "sirv-cli": "^1.0.12",
    "styled-components": "^5.3.5"
  }
}
